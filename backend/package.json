{
  "name": "teevittie",
  "version": "0.0.8",
  "description": "your personal streaming service in a folder",
  "bin": "src/index.js",
  "main": "src/index.js",
  "scripts": {
    "prepublishOnly": "cd .. && npm run build && cd backend",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js ./tmp",
    "dev:f": "nodemon src/index.js",
    "test": "node bin/test.js",
    "test:watch": "node bin/test.js --watch",
    "test:folders": "node scripts/generateTestFolders.js",
    "rm:test:folders": "node scripts/removeTestFolders.js"
  },
  "keywords": [
    "video streaming",
    "netflix-like",
    "folder video streaming",
    "spa"
  ],
  "author": "Vincenzo Ciaccio <vincenzo.ciaccio@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "address": "^1.2.0",
    "cors": "^2.8.5",
    "directory-tree": "^2.2.5",
    "express": "^4.18.1",
    "tiny-hashes": "^1.0.1"
  },
  "devDependencies": {
    "@japa/expect": "^2.0.0",
    "@japa/runner": "^2.1.1",
    "@japa/spec-reporter": "^1.2.0",
    "nodemon": "^2.0.19"
  }
}
